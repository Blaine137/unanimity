{
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "import",
        "jsx-a11y",
        "react-hooks"  
    ],
    "rules": {
		"react/prop-types": "off",
		"no-unused-vars": "warn",
		"no-console": "warn",
        "func-names": "off",
        "linebreak-style": ["error", "windows"],
        "object-curly-newline": "warn",
        "arrow-parens": "off",
        "react/destructuring-assignment": "off",
        "max-len": "off",
        "no-param-reassign": "warn",
        "consistent-return": "warn",
		"react/jsx-filename-extension": "off"
    }
}
