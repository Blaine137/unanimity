{
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"react/prop-types": "off",
		"no-unused-vars": "warn",
		"no-console": "warn",
		"func-names": "off",
		"react/jsx-filename-extension": "off"
    }
}
